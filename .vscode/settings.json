
{
  "[c]": {
    "editor.defaultFormatter": "keyhr.42-c-format",
    "editor.formatOnSave": true
  },
  "editor.insertSpaces": false,
  "editor.tabSize": 4,
  "keybindings": [
    {
      "key": "ctrl+f",
      "command": "-editor.action.startFindReplaceAction",
      "when": "editorFocus"
    }
],
"cSpell.words": [
	"isregularchar",
	"isspecialchar",
	"isvalidvarname",
	"putendl"
],
"workbench.colorCustomizations": {
	"activityBar.background": "#203210",
	"titleBar.activeBackground": "#2D4717",
	"titleBar.activeForeground": "#F9FCF6"
},
"files.associations": {
  "complex": "c"
},
}
